function getCloudIndex()
{
	local CSP=$1
	local CSPIndex=0

	if [ "${CSP}" == "all" ]; then
		# echo "[all CSP regions (AWS, Azure, GCP, Alibaba,  ...)]"
		CSPIndex=0
	elif [ "${CSP}" == "aws" ]; then
		# echo "[AWS]"
		CSPIndex=$IndexAWS
	elif [ "${CSP}" == "alibaba" ]; then
		# echo "[Alibaba]"
		CSPIndex=$IndexAlibaba
	elif [ "${CSP}" == "gcp" ]; then
		# echo "[GCP]"
		CSPIndex=$IndexGCP
	elif [ "${CSP}" == "azure" ]; then
		# echo "[Azure]"
		CSPIndex=$IndexAzure
	elif [ "${CSP}" == "mock" ]; then
		# echo "[Mock driver]"
		CSPIndex=$IndexMock
	elif [ "${CSP}" == "testcloud01" ]; then
		# echo "[TestCloud01 with mock driver]"
		CSPIndex=$IndexTestCloud01
	elif [ "${CSP}" == "testcloud02" ]; then
		# echo "[TestCloud02 with mock driver]"
		CSPIndex=$IndexTestCloud02
	elif [ "${CSP}" == "testcloud03" ]; then
		# echo "[TestCloud03 with mock driver]"
		CSPIndex=$IndexTestCloud03		
	elif [ "${CSP}" == "openstack" ]; then
		# echo "[OpenStack driver]"
		CSPIndex=$IndexOpenstack
	elif [ "${CSP}" == "ncp" ]; then
		# echo "[NCP driver]"
		CSPIndex=$IndexNCP
	elif [ "${CSP}" == "cloudtwin" ]; then
		# echo "[Cloud-Twin driver]"
		CSPIndex=$IndexCloudTwin
	elif [ "${CSP}" == "cloudit" ]; then
		# echo "[Cloudit driver]"
		CSPIndex=$IndexCloudit
	elif [ "${CSP}" == "tencent" ]; then
		# echo "[Tencent driver]"
		CSPIndex=$IndexTencent
	elif [ "${CSP}" == "ktcloud" ]; then
		# echo "[KTcloud driver]"
		CSPIndex=$IndexKTcloud
	elif [ "${CSP}" == "ibm" ]; then
		# echo "[IBM-VPC driver]"
		CSPIndex=$IndexIBMVPC
	elif [ "${CSP}" == "nhncloud" ]; then
		# echo "[NHN Cloud driver]"
		CSPIndex=$IndexNHNcloud
	else
		echo "[No acceptable argument was provided (all, aws, azure, gcp, alibaba, mock, openstack, ...).]"
		exit
	fi

	INDEX=$CSPIndex

}

